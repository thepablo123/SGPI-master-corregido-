<!DOCTYPE html>

<html>
<head>

    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/css/programara.css" />
      <link rel="stylesheet" href="/css/buscare.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    
    <link rel="stylesheet" href="~/css/narvar.css" />
    
  
    <link rel="stylesheet" href="~/SGPI.styles.css" asp-append-version="true" />
    <script href="~/js/narvar"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>
</head>
<body class="body">
    <nav style="background-color: #f6f5f7;" class="navbar navbar-expand-lg navbar-light fixed-top">
        <div class="container">
            <a class="navbar-brand" href="#">SGPI</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item active">
                        <a class="nav-link" href="/Coordinador/BuscarEstudiante">Buscar Estudiante</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Coordinador/ProgramarAsignatura">Programar Asignaturas</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Coordinador/Homologacion">Homologacion</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/Coordinador/EntrevistasAdmisiones"> Entrevistas y Admisiones</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/Coordinador/PagoEstudiante">Pago Estudiante</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/Coordinador/Reportes">Reportes</a>
                    </li>

                </ul>

                <button type="submit" style=" border-radius: 20px;
    border: 1px solid #7e7e7e;
    background-color: #7e7e7e;
    color: #FFFFFF;
    font-size: 12px;
    font-weight: bold;
    padding: 12px 45px;
    letter-spacing: 1px;
    text-transform: uppercase;
    transition: transform 80ms ease-in;
   
    
    position: relative;">
                    Cerrar Sesion
                </button>


            </div>
        </div>
    </nav>
    <div>
        @RenderBody()
    </div>


    <footer>
        Todos los derechos Reservados @DateTime.Now.Year
    </footer>
</body>
</html>
